@using ProjectManagement.DataAccess.Model;
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
@model List<Discipline>

<h1>Student Disciplines</h1>

<div class="card-deck">
    @foreach (var discipline in Model)
    {
        <div class="card">
            <div class="card-body">
                <h5 class="card-title">@discipline.Name</h5>
                <h6 class="card-subtitle">@discipline.Teacher.Name</h6>
            </div>
        </div>
    }
</div>